{
  "name": "@engagifii/enhanced-data-table",
  "version": "1.0.0",
  "description": "A comprehensive, feature-rich data table component with advanced filtering, sorting, pagination, grouping, frozen columns, bulk actions, and custom cell rendering. Includes comprehensive demo showcasing all enterprise-grade features.",
  "private": false,
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./styles": "./dist/styles.css"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "dev": "tsup --watch",
    "demo": "cd demo && (python -m http.server 3001 || python3 -m http.server 3001)",
    "demo:open": "npm run demo &",
    "demo:browse": "open http://localhost:3001 || start http://localhost:3001 || xdg-open http://localhost:3001",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "prepare": "npm run build"
  },
  "keywords": [
    "react",
    "data-table",
    "table",
    "shadcn",
    "ui",
    "components",
    "typescript",
    "tailwind",
    "engagifii"
  ],
  "author": "Engagifii Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/engagifii/enhanced-data-table.git"
  },
  "bugs": {
    "url": "https://github.com/engagifii/enhanced-data-table/issues"
  },
  "homepage": "https://github.com/engagifii/enhanced-data-table#readme",
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "dependencies": {
    "@hello-pangea/dnd": "^16.5.0",
    "@radix-ui/react-checkbox": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-popover": "^1.1.1",
    "@radix-ui/react-scroll-area": "^1.1.0",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-slot": "^1.1.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.462.0",
    "tailwind-merge": "^2.5.2"
  },
  "devDependencies": {
    "@types/node": "^22.5.5",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.11",
    "tsup": "^8.2.4",
    "typescript": "^5.5.3"
  }
}
